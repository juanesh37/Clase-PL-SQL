---creaci贸n de una funci贸n
CREATE OR REPLACE FUNCTION findmin(x in number, y in number)
RETURN number IS
  z number := 0;
BEGIN
  IF x > y THEN
    z := y;
  ELSE
    z := x;
  END IF;
  RETURN z;
END;

--- La impresi贸n de la funci贸n
BEGIN 
  dbms_output.put_line( 'Min 20 - 40: ' || findmin(20,40));
END;
  
